{
	"While Loop": {
		"scope": "paisley",
		"prefix": [
			"while"
		],
		"body": [
			"while {${1:expression}} do",
			"\t$0",
			"end"
		]
	},
	"For Loop": {
		"scope": "paisley",
		"prefix": [
			"for"
		],
		"body": [
			"for ${2:variable} {${1:expression}} do",
			"\t$0",
			"end"
		]
	},
	"If Block": {
		"scope": "paisley",
		"prefix": [
			"if"
		],
		"body": [
			"if {${1:expression}} then",
