{
  "name": "paisley",
  "displayName": "Paisley",
  "description": "Syntax highlighter and snippets for the Paisley scripting language.",
  "version": "0.0.2",
  "publisher": "senor-cluckens",
  "engines": {
    "vscode": ">=1.80.0"
  },
  "contributes": {
    "languages": [{
      "id": "paisley",
      "aliases": ["Paisley", "paisley"],
      "extensions": [".paisley"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "paisley",
      "scopeName": "source.paisley",
      "path": "./syntaxes/paisley.tmLanguage.json"
    }],
    "snippets": [
      {
        "language": "paisley",
        "path": "./snippets/paisley-snippets.json"
      }
    ]
  }
}
